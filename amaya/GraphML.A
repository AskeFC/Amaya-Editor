APPLICATION GraphML;

DEFAULT
   BEGIN
   ElemPaste.Post -> GraphElemPasted;
   PRuleCreate.Pre -> GraphicsPRuleChange;
   PRuleModify.Pre -> GraphicsPRuleChange;
   ElemExtendSelect.Pre -> ExtendSelectGraphMLElement;
   END;

ELEMENTS

   GraphML:
	BEGIN
	ElemNew.Post ->         SVGCreated;
	ElemExport.Pre ->       SetEmptyShapeAttribute;
	END;
   Line_:
	ElemGraphModify.Post -> ControlPointChanged;
   Polyline:
	ElemGraphModify.Post -> ControlPointChanged;
   Polygon:
	ElemGraphModify.Post -> ControlPointChanged;
   Spline:
	ElemGraphModify.Post -> ControlPointChanged;
   ClosedSpline:
	ElemGraphModify.Post -> ControlPointChanged;
   GRAPHICS_UNIT:
	ElemDelete.Post ->      GraphLeafDeleted;
   foreignObject:
	ElemExport.Pre ->       ExportForeignObject;

ATTRIBUTES

   id:
        BEGIN
        AttrCreate.Post -> UpdateAttrID;
        AttrModify.Post -> UpdateAttrID;
        AttrDelete.Post -> UpdateAttrID;
        END;

   class:
        BEGIN
        AttrCreate.Post -> AttrClassChanged;
        AttrModify.Post -> AttrClassChanged;
        AttrDelete.Post -> AttrClassChanged;
        END;
 
   style\240:
        BEGIN
        AttrDelete.Pre ->  UpdateStyleDelete;
        AttrModify.Pre ->  UpdateStyleDelete;
        AttrCreate.Post -> UpdateStylePost;
        AttrModify.Post -> UpdateStylePost;
        END;
 
   Namespace:
	AttrExport.Post -> NameSpaceGenerated;

   position:
	AttrModify.Post -> AttrPositionChanged;
   x:
	AttrModify.Post -> AttrCoordChanged;
   y:
	AttrModify.Post -> AttrCoordChanged;
   IntPosX:
	AttrModify.Post -> AttrIntPosChanged;
   IntPosY:
        AttrModify.Post -> AttrIntPosChanged;
   width_:
	AttrModify.Post -> AttrWidthChanged;
   height_:
	AttrModify.Post -> AttrHeightChanged;
   IntWidth:
        AttrModify.Post -> AttrIntSizeChanged;
   IntHeight:
        AttrModify.Post -> AttrIntSizeChanged;
   fill:
        BEGIN
	AttrModify.Post -> AttrFillStrokeModified;
	AttrCreate.Post -> AttrFillStrokeModified;
	AttrDelete.Pre ->  AttrFillStrokeDelete;
	END;
   stroke:
        BEGIN
	AttrModify.Post -> AttrFillStrokeModified;
	AttrCreate.Post -> AttrFillStrokeModified;
	AttrDelete.Pre ->  AttrFillStrokeDelete;
	END;
   stroke_width:
        BEGIN
	AttrModify.Post -> AttrFillStrokeModified;
	AttrCreate.Post -> AttrFillStrokeModified;
	AttrDelete.Pre ->  AttrFillStrokeDelete;
	END;
   direction:
	BEGIN
	AttrModify.Post -> AttrDirectionModified;
	AttrCreate.Post -> AttrDirectionModified;
	AttrDelete.Post -> AttrDirectionModified;
	END;
   hspace:
	BEGIN
	AttrModify.Post -> AttrSpaceModified;
	AttrCreate.Post -> AttrSpaceModified;
	AttrDelete.Post -> AttrSpaceModified;
	END;
   vspace:
	BEGIN
	AttrModify.Post -> AttrSpaceModified;
	AttrCreate.Post -> AttrSpaceModified;
	AttrDelete.Post -> AttrSpaceModified;
	END;
   arrowhead:
	BEGIN
	AttrModify.Post -> AttrArrowHeadModified;
	AttrCreate.Post -> AttrArrowHeadModified;
	AttrDelete.Post -> AttrArrowHeadModified;
	END;
   points:
	BEGIN
	AttrDelete.Pre ->  DeleteAttrPoints;
	AttrModify.Post -> AttrPointsModified;
	END;
END
