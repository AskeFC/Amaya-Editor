#!/bin/sh
#
# This script builds the Source tar files for Thot Editor
#
#       thoteditor-src-$EDITORVERSION.tar.gz
#
# And the associated Librairies for supported versions.
#
EDITORVERSION=2.1e
THOTDIR=/tahiti/diffusion/opera
DEST=/tahiti/thottape

TAR="tar cvbfh 200"
COMPRESS="gzip -9"

cd $THOTDIR

$TAR - \
