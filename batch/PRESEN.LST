			  'Referred' / 'FirstRef' / 'LastRef' /
			  'ExternalRef' / 'InternalRef' / 'CopyRef' /
			  'AnyAttributes' / 'FirstAttr' / 'LastAttr' /
			  'UserPage' / 'StartPage' / 'ComputedPage' /
			  'Empty' /
			  '(' [ MaxMinValue ] CounterName CounterCond ')' /
			  CondPage '(' CounterName ')' /
			  [ 'Immediately' ] 'Within' [ ParentLevel ] TypeName
			  [ ExtStruct ] /
			  TypeName / AttrName .
	
  149	ParentLevel =	  [ GreaterLess ] RelLevel .
  150	GreaterLess =	  '>' / '<' .
  151	RelLevel =	  NUMBER.
   39	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  147	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
	                  'IN' '[' ['-'] MinCounterInter '.' '.' ['-'] MaxCounterInter ']' .
  148	CondPage =	  'Even' / 'Odd' / 'One' .
  144	Create =	  'Create' / 'CreateBefore' / 'CreateAfter' /
			  'CreateLast' / 'CreateFirst' / 'CreateWith' /
			  'CreateEnclosing'.
			  { Create is obsolete; replaced by CreateFirst }
  152	MaxCounterVal =	  NUMBER .
  153	MinCounterVal =	  NUMBER .
  154	CounterValEqual = NUMBER .
  156	MaxCounterInter = NUMBER .
  155	MinCounterInter = NUMBER .
	
   74	BoxNameList =	  BoxName < ',' BoxName > .
	
   12	TransmitList =	  Transmit < Transmit > .
