   79	LineStyleInherit = Link '=' / 'Solid' / 'Dashed' / 'Dotted' .
	
   80	FloatInherit =    Link '=' / 'Left' / 'Right' / 'None' .
	
   81	ClearInherit =    Link '=' / 'None' / 'Left' / 'Right' / 'Both' .
	
