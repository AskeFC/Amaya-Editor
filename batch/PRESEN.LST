			  ViewRuleList .
   21	AttrName =	  NAME .
  134	FSTypeName =	  [ FirstSec ] TypeName .
  135	AttrRelat =	  '=' AttrValue / '>' [ '-' ] MinVal / '<' [ '-' ] MaxVal /
			  'IN' '[' ['-'] MinInterval '.' '.' ['-'] MaxInterval ']' /
			  'GREATER' AttrName / 'LESS' AttrName / 'EQUAL' AttrName .
  136	AttrValue =	  [ '-' ] ValEqual / TextEqual / AttrVal .
  137	MinVal =	  NUMBER .
  138	MaxVal =	  NUMBER .
  139	MinInterval =	  NUMBER .
  140	MaxInterval =	  NUMBER .
  141	ValEqual =	  NUMBER .
  142	TextEqual =	  STRING .
  143	AttrVal =	  NAME .
	
   73	Creation =	  Create [ 'Repeated' ] .
   49	SeqCondition =	  Condition < 'AND' Condition > .
  145	Condition =	  [ 'NOT' ] [ 'Target' ] ElemCondition .
