  108	DecimPart =	  NUMBER .
  109	Unit =		  'CM' / 'MM' / 'PT' / 'PC' / 'IN' / 'PX' / 'EM' / 'EX' / '%' .
	
   90	Distance =	  [ Sign ] AbsDist .
  111	Sign =		  '+' / '-' .
	
  112	Inheritage =	  Link  InheritVal .
