  143	GreaterLess =	  '>' / '<' .
  144	RelLevel =	  NUMBER.
   39	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  140	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
	                  'IN' '[' ['-'] MinCounterInter '.' '.' ['-'] MaxCounterInter ']' .
  141	CondPage =	  'Even' / 'Odd' / 'One' .
  137	Create =	  'Create' / 'CreateBefore' / 'CreateAfter' /
			  'CreateLast' / 'CreateFirst' / 'CreateWith' /
