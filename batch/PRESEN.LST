GRAMMAR OF FILE PRESEN.LAN


 rule
number

		{ This grammar defines the language P of Thot
		The notation used here is defined in file META.LAN
	
		   ' '  : keyword
	           [ ]  : optional part
	           < >  : optional and repeatable part
	           /    : choice
	
		V. Quint	February 1985	}
	
    1	PresentModel =	  'PRESENTATION' TypeName ';'
			[ 'VIEWS' ViewList ]
			[ 'PRINT' PrintViewList ]
			[ 'COUNTERS' CounterList ]
			[ 'CONST' ConstList ]
			[ 'VAR' VarList ]
			[ 'DEFAULT' ViewRuleList ]
			[ 'BOXES' BoxList ]
			[ 'RULES' PresentList ]
			[ 'ATTRIBUTES' AttrList ]
			[ 'TRANSMIT' TransmitList ]
			  'END' .
	
    2	TypeName =	  NAME .
	
    3	ViewList =	  DclView < ',' DclView > ';' .
   13	DclView =	  ViewName [ 'EXPORT' ] .
   14	ViewName =	  NAME .
	
    4	PrintViewList =	  PrintView < ',' PrintView > ';' .
   15	PrintView =	  ViewName / TypeName .
	
    5	CounterList =	  Counter < Counter > .
   16	Counter =	  CounterName ':' CountFunction ';' .
   17	CounterName =	  NAME . 
	
   18	CountFunction =	  'RANK' 'OF' TypeOrPage [ RelAncestorLevel ] [ 'INIT' AttrName ]
			  [ 'REINIT' AttrName ] /
			  SetRule < SetRule > AddRule < AddRule > [ 'INIT' AttrName ] /
			  'RLevel' 'OF' TypeName .
   20	RelAncestorLevel = [ '-' ] AncestorLevel .
   24	AncestorLevel =	  NUMBER .
   22	SetRule =	  'SET' CountValue 'ON' TypeOrPage .
   23	AddRule =	  'ADD' CountValue 'ON' TypeOrPage .
   19	TypeOrPage =	  'Page' [ '(' ViewName ')' ] / [ '*' ] TypeName .
   25	CountValue =	  NUMBER .
	
    6	ConstList =	  Const < Const > .
   26	Const =		  ConstName '=' ConstType ConstValue ';' .
   27	ConstName =	  NAME .
   28	ConstType =	  'TEXT' [ AlphabetName ] /
			  'SYMBOL' / 'GRAPHICS' / 
			  'PICTURE' [ ImageType ] .
   30	AlphabetName =	  NAME .
   31	ImageType = 	  NUMBER .
   29	ConstValue =	  STRING .
	
    7	VarList =	  Variable < Variable > .
   32	Variable =	  VarName ':' FunctList ';' .
   33	VarName =	  NAME .
   34	FunctList =	  Function < Function > .
   35	Function =	  'DATE' / 'FDATE' /
			  'DocName' / 'DirName' /
			  'ElemName' / 'AttributeName' /
			  'VALUE' '(' CounterAttrPage ',' CounterStyle ')' /
			  ConstType ConstValue /
			  ConstName / AttrName .
   36	CounterAttrPage = 'PageNumber' [ '(' ViewName ')' ] /
			  [ MaxMinValue ] CounterName / AttrName .
   37	CounterStyle =	  'ARABIC' / 'UROMAN' / 'LROMAN' / 'UPPERCASE' /
			  'LOWERCASE' .
	
    9	BoxList =	  Box < Box > .
   39	Box =		  'FORWARD' BoxName ';' /
			  BoxName ':' ViewRuleList .
   40	BoxName =	  NAME .
	
    8	ViewRuleList =	  'BEGIN' < RulesAndCondRules > < ViewRules > 'END' ';' /
			   ViewRules / CondRules / Rule .
   41	RulesAndCondRules = CondRules / Rule .
	
   42	ViewRules =	  'IN' ViewName CondRuleList .
   45	CondRuleList =	  'BEGIN' < RulesAndCondRules > 'END' ';' /
			  CondRules / Rule .
   43	CondRules =	  CondRule  < CondRule > [ 'Otherwise' RuleList ] .
   46	CondRule =	  'IF' SeqCondition RuleList .
	
   47	RuleList =	  'BEGIN' Rule < Rule > 'END' ';' / Rule .
	
   44	Rule =		  Rule1 ';' / Rule2 ';' / Rule3 ';' / Rule4 ';' .
   49	Rule1 =	  	  'VertRef' ':' HorizPosition /
			  'HorizRef' ':' VertPosition /
			  'Height' ':' Dimension /
			  'Width' ':' Dimension /
			  'VertPos' ':' PosH /
			  'HorizPos' ':' PosV /
			  'VertOverflow' ':' Boolean /
			  'HorizOverflow' ':' Boolean /
			  'Justify' ':' BoolInherit /
			  'Hyphenate' ':' BoolInherit /
			  'LineSpacing' ':' DistInherit .
   50	Rule2 =		  'Break' ':' Boolean /
			  { Break is obsolete; replaced by PageBreak }
			  'LineBreak' ':' Boolean /
			  'PageBreak' ':' Boolean /
			  'InLine' ':' Boolean /
			  'Size'  ':' SizeInherit /
			  'Visibility' ':' NbInherit /
			  'Font' ':' NameInherit /
			  'Underline' ':' UnderLineInherit /
			  'Thickness' ':' ThicknessInherit /
			  'Style' ':' StyleInherit /
			  'Indent' ':' DistInherit /
			  'Adjust' ':' AdjustInherit /
			  'NoBreak1' ':' AbsDist /
			  'NoBreak2' ':' AbsDist /
			  'Content' ':' VarConst /
			  'Gather' ':' Boolean .
   51	Rule3 =		  Creation '(' BoxName ')' /
			  'Line' /
			  'NoLine' /
			  'Page' '(' BoxName ')' /
			  [ 'Included' ] 'Column' '(' BoxNameList ')' /
			  'Copy' '(' BoxTypeCopied ')' /
			  'Depth' ':' NbInherit .
   52	Rule4 =		  'LineStyle' ':' LineStyleInherit /
			  'LineWeight' ':' DistInherit /
			  'FillPattern' ':' NameInherit /
			  'Background' ':' NameInherit /
			  'Foreground' ':' NameInherit .
	
   73	LineStyleInherit = Link '=' / 'Solid' / 'Dashed' / 'Dotted' .
	
   72	BoxTypeCopied =	  BoxName [ ExtStruct ] / TypeName [ ExtStruct ] .
   75	ExtStruct =	  '(' TypeName ')' .
	
   56	PosH =		  'nil' / HorizAxis '=' RefVPosition [ 'UserSpecified' ] .
   57	PosV =		  'nil' / VertAxis '=' RefHPosition [ 'UserSpecified' ] .
   77	RefVPosition =	  'Refering' [ '*' ] TypeName '.' HorizAxis [ Distance ] /
			  VertPosition .
   79	RefHPosition =	  'Refering' [ '*' ] TypeName '.' VertAxis [ Distance ] /
			  HorizPosition .
	
   69	VarConst =	  '(' FunctList ')' / ConstType ConstValue /
			  VarName / ConstName / TypeName .
	
   10	PresentList =	  Present < Present > .
   81	Present =	  [ '*' ] [ FirstSec ] TypeName ':' ViewRuleList .
   82	FirstSec =	  'First' / 'Second' .
	
   54	VertPosition =	  Reference '.' HorizAxis [ Distance ] .
   53	HorizPosition =	  Reference '.' VertAxis [ Distance ] .
   83	Reference =	  'Enclosing' [ NBoxType ] /
			  'Enclosed' [ NBoxType ] /
			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
   84	NBoxType =	  [ 'NOT' ] BoxType .
   85	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName / 'AnyElem' / 'AnyBox'.
   78	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   76	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   55	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
   86	HeightWidth =	  'Height' / 'Width' .
   87	Ratio =		  '*' DimRatioA '%' / Distance .
   88	DimRatioA =	  DimRatio / AttrName .
   89	DimRatio =	  NUMBER .
   68	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
   90	IntPartA =	  IntPart / AttrName .
   93	IntPart =	  NUMBER .
   91	DecimPart =	  NUMBER .
   92	Unit =		  'CM' / 'MM' / 'PT' / 'PC' / 'IN' / 'PX' / 'EM' / 'EX' / '%' .
	
   80	Distance =	  [ Sign ] AbsDist .
   94	Sign =		  '+' / '-' .
	
   95	Inheritage =	  Link  InheritVal .
   74	Link =		  'Enclosing' / 'Enclosed' / 'Previous' / 'Creator' /
			  'GrandFather' .
   96	InheritVal =	  '+' PosIntegerA [ 'Max' maximumA ] /
			  '-' NegIntegerA [ 'Min' minimumA ] /
			  '=' .
   97	PosIntegerA =	  PosInteger / AttrName .
  101	PosInteger =	  NUMBER .
   99	NegIntegerA =	  NegInteger / AttrName .
  102	NegInteger =	  NUMBER .
   98	maximumA =	  maximum / AttrName .
  103	maximum =	  NUMBER .
  100	minimumA =	  minimum / AttrName .
  104	minimum =	  NUMBER .
	
   62	NbInherit =	  Integer / AttrName / Inheritage .
  105	Integer =	  NUMBER .
   58	Boolean =	  'Yes' / 'No' .
	
   60	DistInherit =	  Link InheritDist / Distance .
  106	InheritDist =	  '=' / '+' AbsDist / '-' AbsDist .
	
   59	BoolInherit =	  Boolean / Link '=' .
	
   67	AdjustInherit =	  Adjustment / Link '=' .
  107	Adjustment =	  'Left' / 'Right' / 'VMiddle' / 'LeftWithDots' .
	
   61	SizeInherit =	  SizeA [ 'PT' ] / Link  InheritSize .
  109	InheritSize =	  '+' PosSizeA [ 'PT' ] [ 'Max' MaxSizeA ] /
			  '-' NegSizeA [ 'PT' ] [ 'Min' MinSizeA ] /
			  '=' .
  108	SizeA =		  Size / AttrName .
  114	Size =		  NUMBER .
  110	PosSizeA =	  PosSize / AttrName .
  115	PosSize =	  NUMBER .
  112	NegSizeA =	  NegSize / AttrName .
  116	NegSize =	  NUMBER .
  111	MaxSizeA =	  MaxSize / AttrName .
  117	MaxSize =	  NUMBER .
  113	MinSizeA =	  MinSize / AttrName .
  118	MinSize =	  NUMBER .
	
   63	NameInherit =	  Link '=' / FontStyleName .
  119	FontStyleName =	  NAME .
   66	StyleInherit =	  Link '=' / 'Roman' / 'Bold' / 'Italics' / 'BoldItalics' /
			  'Oblique' / 'BoldOblique' .
   64	UnderLineInherit = Link '=' / 'NoUnderline' / 'Underlined' / 'Overlined' /
			  'CrossedOut' .
   65	ThicknessInherit = Link '=' / 'Thick' / 'Thin' .
	
   11	AttrList =	  Attr < Attr > .
  120	Attr =		  AttrName [ '(' FSTypeName ')' ] [ AttrRelat ] ':'
			  ViewRuleList .
   21	AttrName =	  NAME .
  121	FSTypeName =	  [ FirstSec ] TypeName .
  122	AttrRelat =	  '=' AttrValue / '>' [ '-' ] MinVal / '<' [ '-' ] MaxVal /
			  'IN' '[' ['-'] MinInterval '.' '.' ['-'] MaxInterval ']' /
			  'GREATER' AttrName / 'LESS' AttrName / 'EQUAL' AttrName .
  123	AttrValue =	  [ '-' ] ValEqual / TextEqual / AttrVal .
  124	MinVal =	  NUMBER .
  125	MaxVal =	  NUMBER .
  126	MinInterval =	  NUMBER .
  127	MaxInterval =	  NUMBER .
  128	ValEqual =	  NUMBER .
  129	TextEqual =	  STRING .
  130	AttrVal =	  NAME .
	
   70	Creation =	  Create [ 'Repeated' ] .
   48	SeqCondition =	  Condition < 'AND' Condition > .
  132	Condition =	  [ 'NOT' ] [ 'Target' ] ElemCondition .
  133	ElemCondition =	  'First' / 'Last' /
			  'Referred' / 'FirstRef' / 'LastRef' /
			  'ExternalRef' / 'InternalRef' / 'CopyRef' /
			  'AnyAttributes' / 'FirstAttr' / 'LastAttr' /
			  'UserPage' / 'StartPage' / 'ComputedPage' /
			  'Empty' /
			  '(' [ MaxMinValue ] CounterName CounterCond ')' /
			  CondPage '(' CounterName ')' /
			  [ 'Immediately' ] 'Within' [ ParentLevel ] TypeName
			  [ ExtStruct ] /
			  TypeName / AttrName .
	
  136	ParentLevel =	  [ GreaterLess ] RelLevel .
  137	GreaterLess =	  '>' / '<' .
  138	RelLevel =	  NUMBER.
   38	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  134	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
	                  'IN' '[' ['-'] MinCounterInter '.' '.' ['-'] MaxCounterInter ']' .
  135	CondPage =	  'Even' / 'Odd' / 'One' .
  131	Create =	  'Create' / 'CreateBefore' / 'CreateAfter' /
			  'CreateLast' / 'CreateFirst' / 'CreateWith' /
			  'CreateEnclosing'.
			  { Create is obsolete; replaced by CreateFirst }
  139	MaxCounterVal =	  NUMBER .
  140	MinCounterVal =	  NUMBER .
  141	CounterValEqual = NUMBER .
  143	MaxCounterInter = NUMBER .
  142	MinCounterInter = NUMBER .
	
   71	BoxNameList =	  BoxName < ',' BoxName > .
	
   12	TransmitList =	  Transmit < Transmit > .
  144	Transmit =	  TypeOrCounter 'To' ExternalAttr '(' TypeName ')' ';' .
  145	TypeOrCounter =	  CounterName / TypeName .
  146	ExternalAttr =	  NAME .
	
	END
	


