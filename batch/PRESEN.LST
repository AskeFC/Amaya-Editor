			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
  100	NBoxType =	  [ 'NOT' ] BoxType .
  101	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName /
			  'AnyElem' / 'AnyBox' / 'ElemWithAttr' AttrName .
   95	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   93	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   58	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
  102	HeightWidth =	  'Height' / 'Width' .
  103	Ratio =		  '*' DimRatioA '%' / Distance .
  104	DimRatioA =	  DimRatio / AttrName .
  105	DimRatio =	  NUMBER .
   72	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
