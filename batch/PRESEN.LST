GRAMMAR OF FILE PRESEN.LAN


 rule
number

		{ This grammar defines the language P of Thot
		The notation used here is defined in file META.LAN
	
		   ' '  : keyword
	           [ ]  : optional part
	           < >  : optional and repeatable part
	           /    : choice
	
		V. Quint	February 1985	}
	
    1	PresentModel =	  'PRESENTATION' TypeName ';'
			[ 'VIEWS' ViewList ]
			[ 'PRINT' PrintViewList ]
			[ 'COUNTERS' CounterList ]
			[ 'CONST' ConstList ]
			[ 'VAR' VarList ]
			[ 'DEFAULT' ViewRuleList ]
			[ 'BOXES' BoxList ]
			[ 'RULES' PresentList ]
			[ 'ATTRIBUTES' AttrList ]
			[ 'TRANSMIT' TransmitList ]
			  'END' .
	
    2	TypeName =	  NAME .
	
    3	ViewList =	  DclView < ',' DclView > ';' .
   13	DclView =	  ViewName [ 'EXPORT' ] .
   14	ViewName =	  NAME .
	
    4	PrintViewList =	  PrintView < ',' PrintView > ';' .
   15	PrintView =	  ViewName / TypeName .
	
    5	CounterList =	  Counter < Counter > .
   16	Counter =	  CounterName ':' CountFunction ';' .
   17	CounterName =	  NAME . 
	
   18	CountFunction =	  'RANK' 'OF' TypeOrPage [ RelAncestorLevel ]
				[ 'INIT' AttrName ] [ 'REINIT' AttrName ] /
			  SetRule < SetRule > AddRule < AddRule >
				[ 'INIT' AttrName ] /
			  'RLevel' 'OF' TypeName [ CondAttr ] .
   20	RelAncestorLevel = [ '-' ] AncestorLevel .
   25	AncestorLevel =	  NUMBER .
   22	SetRule =	  'SET' CountValue 'ON' TypeOrPage .
   23	AddRule =	  'ADD' CountValue 'ON' TypeOrPage .
   19	TypeOrPage =	  'Page' [ '(' ViewName ')' ] / [ '*' ] TypeName [ CondAttr ] .
   24	CondAttr =	  'With' AttrName / 'Without' AttrName .
   26	CountValue =	  NUMBER .
	
    6	ConstList =	  Const < Const > .
   27	Const =		  ConstName '=' ConstType ConstValue ';' .
   28	ConstName =	  NAME .
   29	ConstType =	  'TEXT' [ AlphabetName ] /
			  'SYMBOL' / 'GRAPHICS' / 
			  'PICTURE' [ ImageType ] .
   31	AlphabetName =	  NAME .
   32	ImageType = 	  NUMBER .
   30	ConstValue =	  STRING .
	
    7	VarList =	  Variable < Variable > .
   33	Variable =	  VarName ':' FunctList ';' .
   34	VarName =	  NAME .
   35	FunctList =	  Function < Function > .
   36	Function =	  'DATE' / 'FDATE' /
			  'DocName' / 'DirName' /
			  'ElemName' / 'AttributeName' /
			  'VALUE' '(' CounterAttrPage ',' CounterStyle ')' /
			  ConstType ConstValue /
			  ConstName / AttrName .
   37	CounterAttrPage = 'PageNumber' [ '(' ViewName ')' ] /
			  [ MaxMinValue ] CounterName / AttrName .
   38	CounterStyle =	  'ARABIC' / 'UROMAN' / 'LROMAN' / 'UPPERCASE' /
			  'LOWERCASE' .
	
    9	BoxList =	  Box < Box > .
   40	Box =		  'FORWARD' BoxName ';' /
			  BoxName ':' ViewRuleList .
   41	BoxName =	  NAME .
	
    8	ViewRuleList =	  'BEGIN' < RulesAndCondRules > < ViewRules > 'END' ';' /
			   ViewRules / CondRules / Rule .
   42	RulesAndCondRules = CondRules / Rule .
	
   43	ViewRules =	  'IN' ViewName CondRuleList .
   46	CondRuleList =	  'BEGIN' < RulesAndCondRules > 'END' ';' /
			  CondRules / Rule .
   44	CondRules =	  CondRule  < CondRule > [ 'Otherwise' RuleList ] .
   47	CondRule =	  'IF' SeqCondition RuleList .
	
   48	RuleList =	  'BEGIN' Rule < Rule > 'END' ';' / Rule .
	
   45	Rule =		  Rule1 ';' / Rule2 ';' / Rule3 ';' / Rule4 ';' / Rule5 ';' .
   50	Rule1 =	  	  'VertRef' ':' HorizPosition /
			  'HorizRef' ':' VertPosition /
			  'Height' ':' Dimension /
			  'Width' ':' Dimension /
			  'VertPos' ':' PosH /
			  'HorizPos' ':' PosV /
			  'VertOverflow' ':' Boolean /
			  'HorizOverflow' ':' Boolean /
			  'Justify' ':' BoolInherit /
			  'Hyphenate' ':' BoolInherit /
			  'LineSpacing' ':' DistInherit .
   51	Rule2 =		  'Break' ':' Boolean /
			  { Break is obsolete; replaced by PageBreak }
			  'LineBreak' ':' Boolean /
			  'PageBreak' ':' Boolean /
			  'InLine' ':' Boolean /
			  'Size'  ':' SizeInherit /
			  'Visibility' ':' NbInherit /
			  'Font' ':' NameInherit /
			  'Underline' ':' UnderLineInherit /
			  'Thickness' ':' ThicknessInherit /
			  'Style' ':' StyleInherit /
			  'Weight' ':' WeightInherit /
			  'Indent' ':' DistInherit /
			  'Adjust' ':' AdjustInherit /
			  'NoBreak1' ':' AbsDist /
			  'NoBreak2' ':' AbsDist /
			  'Content' ':' VarConst /
			  'Gather' ':' Boolean .
   52	Rule3 =		  Creation '(' BoxName ')' /
			  'Line' /
			  'NoLine' /
			  'Page' '(' BoxName ')' /
			  [ 'Included' ] 'Column' '(' BoxNameList ')' /
			  'Copy' '(' BoxTypeCopied ')' /
			  'Depth' ':' NbInherit .
   53	Rule4 =		  'LineStyle' ':' LineStyleInherit /
			  'LineWeight' ':' DistInherit /
			  'FillPattern' ':' NameInherit /
			  'Background' ':' NameInherit /
			  'Foreground' ':' NameInherit /
			  'ShowBox' /
			  'BackgroundPicture' ':' FileName /
			  'PictureMode' ':' PictMode .
	
   54	Rule5 =           'MarginTop' ':' MarginWidth /
	                  'MarginRight' ':' MarginWidth /
	                  'MarginBottom' ':' MarginWidth /
	                  'MarginLeft' ':' MarginWidth /
	                  'PaddingTop' ':' PaddingWidth /
	                  'PaddingRight' ':' PaddingWidth /
	                  'PaddingBottom' ':' PaddingWidth /
	                  'PaddingLeft' ':' PaddingWidth /
	                  'BorderTopWidth' ':' BorderWidth /
	                  'BorderRightWidth' ':' BorderWidth /
	                  'BorderBottomWidth' ':' BorderWidth /
	                  'BorderLeftWidth' ':' BorderWidth /
	                  'BorderTopColor' ':' BorderColor /
	                  'BorderRightColor' ':' BorderColor /
	                  'BorderBottomColor' ':' BorderColor /
	                  'BorderLeftColor' ':' BorderColor /
	                  'BorderTopStyle' ':' BorderStyle /
	                  'BorderRightStyle' ':' BorderStyle /
	                  'BorderBottomStyle' ':' BorderStyle /
	                  'BorderLeftStyle' ':' BorderStyle .
	
   79	MarginWidth =     'Enclosing' '=' / Distance .	{ add 'auto' }
   80	PaddingWidth =    'Enclosing' '=' / Distance .
   81	BorderWidth =     'Enclosing' '=' / 'Thin' / 'Medium' / 'Thick' / Distance .
   82	BorderColor =     'Enclosing' '=' / ColorName .	{ add 'Transparent' }
   83	BorderStyle =     'Enclosing' '=' /
	                  'None' / 'Hidden' / 'Dotted' / 'Dashed' / 'Solid' /
	                  'Double' / 'Groove' / 'Ridge' / 'Inset' / 'Outset' .
   85	ColorName =       NAME .
	
   77	FileName =	  STRING .
   78	PictMode =	  'NormalSize' / 'Scale' / 'RepeatXY' / 'RepeatX' / 'RepeatY' .
	
   76	LineStyleInherit = Link '=' / 'Solid' / 'Dashed' / 'Dotted' .
	
   75	BoxTypeCopied =	  BoxName [ ExtStruct ] / TypeName [ ExtStruct ] .
   87	ExtStruct =	  '(' TypeName ')' .
	
   58	PosH =		  'nil' / HorizAxis '=' RefVPosition [ 'UserSpecified' ] .
   59	PosV =		  'nil' / VertAxis '=' RefHPosition [ 'UserSpecified' ] .
   89	RefVPosition =	  'Refering' [ '*' ] TypeName '.' HorizAxis [ Distance ] /
			  VertPosition .
   91	RefHPosition =	  'Refering' [ '*' ] TypeName '.' VertAxis [ Distance ] /
			  HorizPosition .
	
   72	VarConst =	  '(' FunctList ')' / ConstType ConstValue /
			  VarName / ConstName / TypeName .
	
   10	PresentList =	  Present < Present > .
   92	Present =	  [ '*' ] [ FirstSec ] TypeName ':' ViewRuleList .
   93	FirstSec =	  'First' / 'Second' .
	
   56	VertPosition =	  Reference '.' HorizAxis [ Distance ] .
   55	HorizPosition =	  Reference '.' VertAxis [ Distance ] .
   94	Reference =	  'Enclosing' [ NBoxType ] /
			  'Enclosed' [ NBoxType ] /
			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
   95	NBoxType =	  [ 'NOT' ] BoxType .
   96	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName /
			  'AnyElem' / 'AnyBox' / 'ElemWithAttr' AttrName .
   90	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   88	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   57	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
   97	HeightWidth =	  'Height' / 'Width' .
   98	Ratio =		  '*' DimRatioA '%' / Distance .
   99	DimRatioA =	  DimRatio / AttrName .
  100	DimRatio =	  NUMBER .
   71	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
  101	IntPartA =	  IntPart / AttrName .
  104	IntPart =	  NUMBER .
  102	DecimPart =	  NUMBER .
  103	Unit =		  'CM' / 'MM' / 'PT' / 'PC' / 'IN' / 'PX' / 'EM' / 'EX' / '%' .
	
   84	Distance =	  [ Sign ] AbsDist .
  105	Sign =		  '+' / '-' .
	
  106	Inheritage =	  Link  InheritVal .
   86	Link =		  'Enclosing' / 'Enclosed' / 'Previous' / 'Creator' /
			  'GrandFather' .
  107	InheritVal =	  '+' PosIntegerA [ 'Max' maximumA ] /
			  '-' NegIntegerA [ 'Min' minimumA ] /
			  '=' .
  108	PosIntegerA =	  PosInteger / AttrName .
  112	PosInteger =	  NUMBER .
  110	NegIntegerA =	  NegInteger / AttrName .
  113	NegInteger =	  NUMBER .
  109	maximumA =	  maximum / AttrName .
  114	maximum =	  NUMBER .
  111	minimumA =	  minimum / AttrName .
  115	minimum =	  NUMBER .
	
   64	NbInherit =	  Integer / AttrName / Inheritage .
  116	Integer =	  NUMBER .
   60	Boolean =	  'Yes' / 'No' .
	
   62	DistInherit =	  Link InheritDist / Distance .
  117	InheritDist =	  '=' / '+' AbsDist / '-' AbsDist .
	
   61	BoolInherit =	  Boolean / Link '=' .
	
   70	AdjustInherit =	  Adjustment / Link '=' .
  118	Adjustment =	  'Left' / 'Right' / 'VMiddle' / 'LeftWithDots' .
	
   63	SizeInherit =	  SizeA [ 'PT' ] / Link  InheritSize .
  120	InheritSize =	  '+' PosSizeA [ 'PT' ] [ 'Max' MaxSizeA ] /
			  '-' NegSizeA [ 'PT' ] [ 'Min' MinSizeA ] /
			  '*' PercentSizeA '%' /
			  '=' .
  119	SizeA =		  Size / AttrName .
  126	Size =		  NUMBER .
  121	PosSizeA =	  PosSize / AttrName .
  127	PosSize =	  NUMBER .
  123	NegSizeA =	  NegSize / AttrName .
  128	NegSize =	  NUMBER .
  122	MaxSizeA =	  MaxSize / AttrName .
  129	MaxSize =	  NUMBER .
  124	MinSizeA =	  MinSize / AttrName .
  130	MinSize =	  NUMBER .
  125	PercentSizeA =	  PercentSize / AttrName .
