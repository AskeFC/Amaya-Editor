  152	ParentLevel =	  [ GreaterLess ] RelLevel .
  153	GreaterLess =	  '>' / '<' .
  154	RelLevel =	  NUMBER.
   40	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  150	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
