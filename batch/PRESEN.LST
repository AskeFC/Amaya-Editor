			  VertPosition .
   95	RefHPosition =	  'Refering' [ '*' ] TypeName '.' VertAxis [ Distance ] /
			  HorizPosition .
	
   73	VarConst =	  '(' FunctList ')' / ConstType ConstValue /
			  VarName / ConstName .
	
   10	PresentList =	  Present < Present > .
   96	Present =	  [ '*' ] [ FirstSec ] TypeName ':' ViewRuleList .
   97	FirstSec =	  'First' / 'Second' .
	
   57	VertPosition =	  Reference '.' HorizAxis [ Distance ] .
   56	HorizPosition =	  Reference '.' VertAxis [ Distance ] .
   98	Reference =	  'Enclosing' [ NBoxType ] /
			  'Enclosed' [ NBoxType ] /
			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
   99	NBoxType =	  [ 'NOT' ] BoxType .
  100	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName /
			  'AnyElem' / 'AnyBox' / 'ElemWithAttr' AttrName .
   94	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   92	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   58	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
  101	HeightWidth =	  'Height' / 'Width' .
  102	Ratio =		  '*' DimRatioA '%' / Distance .
  103	DimRatioA =	  DimRatio / AttrName .
  104	DimRatio =	  NUMBER .
   72	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
  105	IntPartA =	  IntPart / AttrName .
  108	IntPart =	  NUMBER .
  106	DecimPart =	  NUMBER .
  107	Unit =		  'CM' / 'MM' / 'PT' / 'PC' / 'IN' / 'PX' / 'EM' / 'EX' / '%' .
	
   88	Distance =	  [ Sign ] AbsDist .
  109	Sign =		  '+' / '-' .
	
  110	Inheritage =	  Link  InheritVal .
   90	Link =		  'Enclosing' / 'Enclosed' / 'Previous' / 'Creator' /
			  'GrandFather' .
  111	InheritVal =	  '+' PosIntegerA [ 'Max' maximumA ] /
			  '-' NegIntegerA [ 'Min' minimumA ] /
			  '=' .
