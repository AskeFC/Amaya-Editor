   91	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   89	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   57	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
   98	HeightWidth =	  'Height' / 'Width' .
   99	Ratio =		  '*' DimRatioA '%' / Distance .
  100	DimRatioA =	  DimRatio / AttrName .
  101	DimRatio =	  NUMBER .
   71	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
