    1	PresentModel =	  'PRESENTATION' TypeName ';'
			[ 'VIEWS' ViewList ]
			[ 'PRINT' PrintViewList ]
			[ 'COUNTERS' CounterList ]
			[ 'CONST' ConstList ]
			[ 'VAR' VarList ]
			[ 'DEFAULT' ViewRuleList ]
			[ 'BOXES' BoxList ]
			[ 'RULES' PresentList ]
			[ 'ATTRIBUTES' AttrList ]
			[ 'TRANSMIT' TransmitList ]
			  'END' .
	
    2	TypeName =	  NAME .
	
    3	ViewList =	  DclView < ',' DclView > ';' .
   13	DclView =	  ViewName [ 'EXPORT' ] .
   14	ViewName =	  NAME .
	
    4	PrintViewList =	  PrintView < ',' PrintView > ';' .
   15	PrintView =	  ViewName / TypeName .
	
    5	CounterList =	  Counter < Counter > .
   16	Counter =	  CounterName ':' CountFunction ';' .
   17	CounterName =	  NAME . 
	
   18	CountFunction =	  'RANK' 'OF' TypeOrPage [ RelAncestorLevel ]
				[ 'INIT' AttrName ] [ 'REINIT' AttrName ] /
			  SetRule < SetRule > AddRule < AddRule >
				[ 'INIT' AttrName ] /
			  'RLevel' 'OF' TypeName [ CondAttr ] .
   20	RelAncestorLevel = [ '-' ] AncestorLevel .
   25	AncestorLevel =	  NUMBER .
   22	SetRule =	  'SET' CountValue 'ON' TypeOrPage .
   23	AddRule =	  'ADD' CountValue 'ON' TypeOrPage .
   19	TypeOrPage =	  'Page' [ '(' ViewName ')' ] / [ '*' ] TypeName [ CondAttr ] .
   24	CondAttr =	  'With' AttrName / 'Without' AttrName .
   26	CountValue =	  NUMBER .
	
    6	ConstList =	  Const < Const > .
   27	Const =		  ConstName '=' ConstType ConstValue ';' .
   28	ConstName =	  NAME .
   29	ConstType =	  'TEXT' [ AlphabetName ] /
			  'SYMBOL' / 'GRAPHICS' / 
			  'PICTURE' [ ImageType ] .
   31	AlphabetName =	  NAME .
   32	ImageType = 	  NUMBER .
   30	ConstValue =	  STRING .
	
    7	VarList =	  Variable < Variable > .
   33	Variable =	  VarName ':' FunctList ';' .
   34	VarName =	  NAME .
   35	FunctList =	  Function < Function > .
   36	Function =	  'DATE' / 'FDATE' /
			  'DocName' / 'DirName' /
			  'ElemName' / 'AttributeName' /
			  'VALUE' '(' CounterAttrPage ',' CounterStyle ')' /
			  ConstType ConstValue /
			  ConstName / AttrName .
   37	CounterAttrPage = 'PageNumber' [ '(' ViewName ')' ] /
			  [ MaxMinValue ] CounterName / AttrName .
   38	CounterStyle =	  'ARABIC' / 'UROMAN' / 'LROMAN' / 'UPPERCASE' /
			  'LOWERCASE' .
	
    9	BoxList =	  Box < Box > .
   40	Box =		  'FORWARD' BoxName ';' /
			  BoxName ':' ViewRuleList .
   41	BoxName =	  NAME .
	
    8	ViewRuleList =	  'BEGIN' < RulesAndCondRules > < ViewRules > 'END' ';' /
			   ViewRules / CondRules / Rule .
   42	RulesAndCondRules = CondRules / Rule .
	
   43	ViewRules =	  'IN' ViewName CondRuleList .
   46	CondRuleList =	  'BEGIN' < RulesAndCondRules > 'END' ';' /
			  CondRules / Rule .
   44	CondRules =	  CondRule  < CondRule > [ 'Otherwise' RuleList ] .
   47	CondRule =	  'IF' SeqCondition RuleList .
	
   48	RuleList =	  'BEGIN' Rule < Rule > 'END' ';' / Rule .
	
   45	Rule =		  Rule1 ';' / Rule2 ';' / Rule3 ';' / Rule4 ';' .
   50	Rule1 =	  	  'VertRef' ':' HorizPosition /
			  'HorizRef' ':' VertPosition /
			  'Height' ':' Dimension /
			  'Width' ':' Dimension /
			  'VertPos' ':' PosH /
			  'HorizPos' ':' PosV /
			  'VertOverflow' ':' Boolean /
			  'HorizOverflow' ':' Boolean /
			  'Justify' ':' BoolInherit /
			  'Hyphenate' ':' BoolInherit /
			  'LineSpacing' ':' DistInherit .
   51	Rule2 =		  'Break' ':' Boolean /
			  { Break is obsolete; replaced by PageBreak }
			  'LineBreak' ':' Boolean /
			  'PageBreak' ':' Boolean /
			  'InLine' ':' Boolean /
			  'Size'  ':' SizeInherit /
			  'Visibility' ':' NbInherit /
			  'Font' ':' NameInherit /
			  'Underline' ':' UnderLineInherit /
			  'Thickness' ':' ThicknessInherit /
			  'Style' ':' StyleInherit /
			  'Indent' ':' DistInherit /
			  'Adjust' ':' AdjustInherit /
			  'NoBreak1' ':' AbsDist /
			  'NoBreak2' ':' AbsDist /
			  'Content' ':' VarConst /
			  'Gather' ':' Boolean .
   52	Rule3 =		  Creation '(' BoxName ')' /
			  'Line' /
			  'NoLine' /
			  'Page' '(' BoxName ')' /
			  [ 'Included' ] 'Column' '(' BoxNameList ')' /
			  'Copy' '(' BoxTypeCopied ')' /
			  'Depth' ':' NbInherit .
   53	Rule4 =		  'LineStyle' ':' LineStyleInherit /
			  'LineWeight' ':' DistInherit /
			  'FillPattern' ':' NameInherit /
			  'Background' ':' NameInherit /
			  'Foreground' ':' NameInherit /
			  'ShowBox' /
			  'BackgroundPicture' ':' FileName /
			  'PictureMode' ':' PictMode .
	
   75	FileName =	  STRING .
   76	PictMode =	  'NormalSize' / 'Scale' / 'RepeatXY' / 'RepeatX' / 'RepeatY' .
	
   74	LineStyleInherit = Link '=' / 'Solid' / 'Dashed' / 'Dotted' .
	
   73	BoxTypeCopied =	  BoxName [ ExtStruct ] / TypeName [ ExtStruct ] .
   78	ExtStruct =	  '(' TypeName ')' .
	
   57	PosH =		  'nil' / HorizAxis '=' RefVPosition [ 'UserSpecified' ] .
   58	PosV =		  'nil' / VertAxis '=' RefHPosition [ 'UserSpecified' ] .
   80	RefVPosition =	  'Refering' [ '*' ] TypeName '.' HorizAxis [ Distance ] /
			  VertPosition .
   82	RefHPosition =	  'Refering' [ '*' ] TypeName '.' VertAxis [ Distance ] /
			  HorizPosition .
	
   70	VarConst =	  '(' FunctList ')' / ConstType ConstValue /
			  VarName / ConstName / TypeName .
	
   10	PresentList =	  Present < Present > .
   84	Present =	  [ '*' ] [ FirstSec ] TypeName ':' ViewRuleList .
   85	FirstSec =	  'First' / 'Second' .
	
   55	VertPosition =	  Reference '.' HorizAxis [ Distance ] .
   54	HorizPosition =	  Reference '.' VertAxis [ Distance ] .
   86	Reference =	  'Enclosing' [ NBoxType ] /
			  'Enclosed' [ NBoxType ] /
			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
   87	NBoxType =	  [ 'NOT' ] BoxType .
   88	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName /
			  'AnyElem' / 'AnyBox' / 'ElemWithAttr' AttrName .
   81	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   79	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   56	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
   89	HeightWidth =	  'Height' / 'Width' .
   90	Ratio =		  '*' DimRatioA '%' / Distance .
   91	DimRatioA =	  DimRatio / AttrName .
   92	DimRatio =	  NUMBER .
   69	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
   93	IntPartA =	  IntPart / AttrName .
   96	IntPart =	  NUMBER .
   94	DecimPart =	  NUMBER .
   95	Unit =		  'CM' / 'MM' / 'PT' / 'PC' / 'IN' / 'PX' / 'EM' / 'EX' / '%' .
	
   83	Distance =	  [ Sign ] AbsDist .
   97	Sign =		  '+' / '-' .
	
   98	Inheritage =	  Link  InheritVal .
   77	Link =		  'Enclosing' / 'Enclosed' / 'Previous' / 'Creator' /
			  'GrandFather' .
   99	InheritVal =	  '+' PosIntegerA [ 'Max' maximumA ] /
			  '-' NegIntegerA [ 'Min' minimumA ] /
			  '=' .
  100	PosIntegerA =	  PosInteger / AttrName .
  104	PosInteger =	  NUMBER .
  102	NegIntegerA =	  NegInteger / AttrName .
  105	NegInteger =	  NUMBER .
  101	maximumA =	  maximum / AttrName .
  106	maximum =	  NUMBER .
  103	minimumA =	  minimum / AttrName .
  107	minimum =	  NUMBER .
	
   63	NbInherit =	  Integer / AttrName / Inheritage .
  108	Integer =	  NUMBER .
   59	Boolean =	  'Yes' / 'No' .
	
   61	DistInherit =	  Link InheritDist / Distance .
  109	InheritDist =	  '=' / '+' AbsDist / '-' AbsDist .
	
   60	BoolInherit =	  Boolean / Link '=' .
	
   68	AdjustInherit =	  Adjustment / Link '=' .
  110	Adjustment =	  'Left' / 'Right' / 'VMiddle' / 'LeftWithDots' .
	
   62	SizeInherit =	  SizeA [ 'PT' ] / Link  InheritSize .
  112	InheritSize =	  '+' PosSizeA [ 'PT' ] [ 'Max' MaxSizeA ] /
			  '-' NegSizeA [ 'PT' ] [ 'Min' MinSizeA ] /
			  '=' .
  111	SizeA =		  Size / AttrName .
  117	Size =		  NUMBER .
  113	PosSizeA =	  PosSize / AttrName .
  118	PosSize =	  NUMBER .
  115	NegSizeA =	  NegSize / AttrName .
  119	NegSize =	  NUMBER .
  114	MaxSizeA =	  MaxSize / AttrName .
  120	MaxSize =	  NUMBER .
  116	MinSizeA =	  MinSize / AttrName .
  121	MinSize =	  NUMBER .
	
   64	NameInherit =	  Link '=' / FontStyleName .
  122	FontStyleName =	  NAME .
   67	StyleInherit =	  Link '=' / 'Roman' / 'Bold' / 'Italics' / 'BoldItalics' /
			  'Oblique' / 'BoldOblique' .
   65	UnderLineInherit = Link '=' / 'NoUnderline' / 'Underlined' / 'Overlined' /
			  'CrossedOut' .
   66	ThicknessInherit = Link '=' / 'Thick' / 'Thin' .
	
   11	AttrList =	  Attr < Attr > .
  123	Attr =		  AttrName [ '(' FSTypeName ')' ] [ AttrRelat ] ':'
			  ViewRuleList .
   21	AttrName =	  NAME .
  124	FSTypeName =	  [ FirstSec ] TypeName .
  125	AttrRelat =	  '=' AttrValue / '>' [ '-' ] MinVal / '<' [ '-' ] MaxVal /
			  'IN' '[' ['-'] MinInterval '.' '.' ['-'] MaxInterval ']' /
			  'GREATER' AttrName / 'LESS' AttrName / 'EQUAL' AttrName .
  126	AttrValue =	  [ '-' ] ValEqual / TextEqual / AttrVal .
  127	MinVal =	  NUMBER .
  128	MaxVal =	  NUMBER .
  129	MinInterval =	  NUMBER .
  130	MaxInterval =	  NUMBER .
  131	ValEqual =	  NUMBER .
  132	TextEqual =	  STRING .
  133	AttrVal =	  NAME .
	
   71	Creation =	  Create [ 'Repeated' ] .
   49	SeqCondition =	  Condition < 'AND' Condition > .
  135	Condition =	  [ 'NOT' ] [ 'Target' ] ElemCondition .
  136	ElemCondition =	  'First' / 'Last' /
			  'Referred' / 'FirstRef' / 'LastRef' /
			  'ExternalRef' / 'InternalRef' / 'CopyRef' /
			  'AnyAttributes' / 'FirstAttr' / 'LastAttr' /
			  'UserPage' / 'StartPage' / 'ComputedPage' /
			  'Empty' /
			  '(' [ MaxMinValue ] CounterName CounterCond ')' /
			  CondPage '(' CounterName ')' /
			  [ 'Immediately' ] 'Within' [ ParentLevel ] TypeName
			  [ ExtStruct ] /
			  TypeName / AttrName .
	
  139	ParentLevel =	  [ GreaterLess ] RelLevel .
  140	GreaterLess =	  '>' / '<' .
  141	RelLevel =	  NUMBER.
   39	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  137	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
	                  'IN' '[' ['-'] MinCounterInter '.' '.' ['-'] MaxCounterInter ']' .
  138	CondPage =	  'Even' / 'Odd' / 'One' .
  134	Create =	  'Create' / 'CreateBefore' / 'CreateAfter' /
			  'CreateLast' / 'CreateFirst' / 'CreateWith' /
			  'CreateEnclosing'.
			  { Create is obsolete; replaced by CreateFirst }
  142	MaxCounterVal =	  NUMBER .
  143	MinCounterVal =	  NUMBER .
  144	CounterValEqual = NUMBER .
  146	MaxCounterInter = NUMBER .
  145	MinCounterInter = NUMBER .
	
   72	BoxNameList =	  BoxName < ',' BoxName > .
	
   12	TransmitList =	  Transmit < Transmit > .
  147	Transmit =	  TypeOrCounter 'To' ExternalAttr '(' TypeName ')' ';' .
  148	TypeOrCounter =	  CounterName / TypeName .
  149	ExternalAttr =	  NAME .
	
	END
	


