  157	RelLevel =	  NUMBER.
   40	MaxMinValue =	  'MaxRangeVal' / 'MinRangeVal' .
  153	CounterCond =	  '<' MaxCounterVal / '>' MinCounterVal / '=' CounterValEqual / 
