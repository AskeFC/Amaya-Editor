   99	FirstSec =	  'First' / 'Second' .
	
   57	VertPosition =	  Reference '.' HorizAxis [ Distance ] .
   56	HorizPosition =	  Reference '.' VertAxis [ Distance ] .
  100	Reference =	  'Enclosing' [ NBoxType ] /
			  'Enclosed' [ NBoxType ] /
			  'Previous' [ NBoxType ] /
			  'Next' [ NBoxType ] /
			  'Refered' [ NBoxType ] /
			  'Referred' [ NBoxType ] /
			  'Creator' /
			  'Root' /
			  '*' /
			  BoxType .
  101	NBoxType =	  [ 'NOT' ] BoxType .
  102	BoxType =	  BoxName / [ '*' ] [ FirstSec ] TypeName /
			  'AnyElem' / 'AnyBox' / 'ElemWithAttr' AttrName .
   96	VertAxis =	  'Left' / 'VMiddle' / 'VRef' / 'Right' . 
   94	HorizAxis =	  'Top' / 'HMiddle' /'HRef' / 'Bottom' .
	
   58	Dimension =	  Reference '.' HeightWidth [ Ratio ] [ 'Min' ] / 
			  AbsDist [ 'UserSpecified' ] [ 'Min' ] /
			  PosH / PosV .
  103	HeightWidth =	  'Height' / 'Width' .
  104	Ratio =		  '*' DimRatioA '%' / Distance .
  105	DimRatioA =	  DimRatio / AttrName .
  106	DimRatio =	  NUMBER .
   72	AbsDist =	  IntPartA [ '.' DecimPart ] [ Unit ] .
