#!/bin/sh
#
# This script builds the Source tar files for Amaya
#
#       amaya-src-$AMAYAVERSION.tgz
#
# And the associated Librairies for supported versions.
#
#       amaya-libs-$MACH-$AMAYAVERSION.tgz
#
AMAYAVERSION=v.r
THOTDIR=$PWD/../..
DEST=$HOME

TAR="tar cvbfh 200"
COMPRESS="gzip -9"
cd $THOTDIR
echo "Generating the source tar file from " $THOTDIR

$TAR - \
Amaya/README.amaya \
Amaya/README \
Amaya/README.gl \
Amaya/config.guess \
Amaya/config.h.in \
