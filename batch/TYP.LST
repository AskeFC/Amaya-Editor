GRAMMAR OF FILE TYP.LAN


 rule
number

    1	SchemaTypo =	'TYPOGRAPHY' IdentType ';'
		      [ 'ALINEA' SuiteType]
		      [ 'FUNCTION' SuiteFunct ]
		      [ 'COMPOSITION' SuiteMod ]
		      [ 'ELEMENT' SuiteTypo ]
		      [ 'ATTR' SuiteTypoAttr ]
			'END' . 
	
    3	SuiteType =	IdentType < ',' IdentType > ';' .
    4	SuiteFunct =	Funct < Funct > .
    8	Funct =		TypeFunct ':' IdentFunct { '=' DefFunct } ';' .
	{ DefFunct =	'BEGIN' Cmd_List 'END' . }
    9	TypeFunct =	'Capitalization' / 'Punctuation' / 'InsertPair' /
			'Distance' / 'Exponent' / 'Abbreviation' /
			'Attribute' / 'Word' / 'SpaceTable' .
   10	IdentFunct =	NAME .
	
   11	RegleTypo =	[ 'IF' '(' SuiteCondition ')' ] SuiteParam ';' .
   13	SuiteParam =	'BEGIN' ParamTypo < ParamTypo > 'END' ';' / ParamTypo .
   14	ParamTypo =	TypeFunct ':' IdFunct / IdentMod .
   15	IdFunct =	IdentFunct / 'FunctNil' .
	
   12	SuiteCondition = Condition < 'AND' Condition > .
   17	Condition =	[ 'NOT' ] ConditionElem .
   18	ConditionElem =	'First' / 
			'Last' / 
			 RelType OpType IdentType /
			'IN' IdentType /
			'LANGUAGE' '=' Language /
			'IS' LexicalUnit .
   20	OpType =	'IN' / '=' .
   19	RelType =	'Next' / 'Previous' .
   22	LexicalUnit =	NAME .
   21	Language =	NAME .
	
    5	SuiteMod =	Mod < Mod > .
   23	Mod =		IdentMod '=' SuiteRegles . 
   16	IdentMod =	NAME .
   24	SuiteRegles =	'BEGIN' RegleTypo < RegleTypo > 'END' ';' / RegleTypo .
	
    6	SuiteTypo =	Typo < Typo > .
   25	Typo =		IdentType ':' SuiteRegles .
	
    7	SuiteTypoAttr =	TransAttr < TransAttr > .
   26	TransAttr =	AttrIdent [ '(' IdentType ')' ] [ AttrRelat ] ':'
			SuiteRegles .
   27	AttrIdent =	NAME .
   28	AttrRelat =	'=' AttrValue / '>' [ '-' ] MinVal / '<' [ '-' ] MaxVal /
			'IN' '[' ['-'] MinInterval '.' '.' ['-'] MaxInterval ']' .
   29	AttrValue =	[ '-' ] ValEqual / TextEqual / AttrValIdent .
   30	MinVal =	NUMBER .
   31	MaxVal =	NUMBER .
   32	MinInterval =	NUMBER .
   33	MaxInterval =	NUMBER .
   34	ValEqual =	NUMBER .
   35	TextEqual =	STRING .
   36	AttrValIdent =	NAME .
    2	IdentType =	NAME .
	
	END
	


