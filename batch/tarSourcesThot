for MACH in $PLATFORMS
do
    $TAR - \
    Thot/$MACH/Makefile.orig \
    Thot/$MACH/config.h \
    Thot/$MACH/libjpeg.a \
    Thot/$MACH/libpng.a \
    Thot/$MACH/libz.a \
    | $COMPRESS  > $DEST/thot-libs-$MACH-$VERSION.tar.gz
done

$TAR - \
Thot/dicopar/Eprinc.dic \
| $COMPRESS  > $DEST/English.tar.gz 

$TAR - \
Thot/dicopar/Fprinc.dic \
| $COMPRESS  > $DEST/French.tar.gz 

