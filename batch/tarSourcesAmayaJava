#!/bin/sh
#
# This script builds the Source tar files for Amaya-Java
#
#       amaya-java-src-$AMAYAVERSION.tar.gz
#
#
AMAYAVERSION=1.1c
THOTDIR=/nfs/u/veillard/src/Thot
DEST=/u/veillard

TAR="tar cvbfh 200"
COMPRESS="gzip -9"

cd $THOTDIR
cd ..

