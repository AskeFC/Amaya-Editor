for MACH in $PLATFORMS
do
    $TAR - \
    Amaya/$MACH/Makefile.orig \
    Amaya/$MACH/config.h \
    Amaya/$MACH/libwww.a \
    Amaya/$MACH/libjpeg.a \
    Amaya/$MACH/libpng.a \
    Amaya/$MACH/libz.a \
    | $COMPRESS  > $DEST/amaya-libs-$MACH-$VERSION.tar.gz
done

