@VPATHOPT@= @srcdir@
THOTDIR = @top_srcdir@

include ../../Options

LIBS	= @LIBS@ @EXTRA_LIBS@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@


all ilustub : ../../bin ../../bin/ilustub

../../bin:
	@($(MKDIR) ../../bin)

../../bin/ilustub : ilustub.o
	$(LD) $(LDFLAGS) -o $@ ilustub.o

ilustub.o: @srcdir@/ilustub.c
	$(CC) $(CFLAGS) -I../.. -c @srcdir@/ilustub.c

clean :
	$(RM) -f ../../bin/ilustub *.o API*

install : all

uninstall :

test: all
	../../bin/ilustub -T @top_srcdir@/ilulib/IluTypes.def -M thotlib_APIContent \
	           @top_srcdir@/thotlib/include/content.h 
	../../bin/ilustub -T @top_srcdir@/ilulib/IluTypes.def -M thotlib_APITree \
	           @top_srcdir@/thotlib/include/tree.h 
